SELECT I.SUMMARY, I2.NAME FROM ISSUES I
JOIN ISSUESLISTS I2 on I2.ID = I.ISSUESLIST_ID;

SELECT I.SUMMARY, U.FIRSTNAME, U.LASTNAME FROM ISSUES I
JOIN USERS U ON I.UNSIGNEDUSER_ID_ASSIGNEDTO = U.ID;

SELECT U.FIRSTNAME, U.LASTNAME FROM users U
JOIN ISSUES I on U.ID = I.UNSIGNEDUSER_ID_ASSIGNEDTO
HAVING SUMMARY NOT NULL;